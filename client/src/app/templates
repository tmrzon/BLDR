
///////////////////////////////
CreateSection1000() {
    let section = new Section(0, 'Client Say', '', '5fd7511ccd37f17aa8a7f729');
    this.lastCol = section.item.rows[0].cols[0];
    this.lastCol.styles["background-image"] = "url(assets/img/eberhard-grossgasteiger-S-2Ukb_VqpA-unsplash.jpg)"
    this.lastCol.styles["min-height"] = "100vh"
    let grid = DbService.createGrid(null, 1, 1);
    grid.parentCol = section.item.rows[0].cols[0];
    let h1 = new Item('h1', {}, {
      'simpleStyles': {
        'color': '#292929',
        'letter-spacing': '-1px',
        'opacity': '1',
        'border': 'none',
        'text-align': 'center',
        'font': 'normal normal medium 41px/46px Rubik'
      },
    }, null, [], [], 'What our Client Say');
    h1.parentCol = grid.rows[0].cols[0];
    let grid2 = DbService.createGrid(null, 4, 4);
    grid2.parentCol = grid.rows[0].cols[0];
    grid2.rows[0].cols[0].styles['min-height'] = '376px'
    grid2.rows[0].cols[1].styles['min-height'] = '376px'
    grid2.rows[0].cols[2].styles['min-height'] = '376px'
    grid2.rows[0].cols[3].styles['min-height'] = '376px'
    let img, h5, para, p;
    let counter = 0;
    let source = [
      { img: 'assets/img/circle-200.png', h5: 'Patrick Peters', p: 'FairyTale', g: '"Donec facilisis tortor ut augue \r\nlacinia, at viverra est semper. Sed\r\n sapien metus, scelerisque nec \r\npharetra id, tempor a tortor." ' },
      { img: 'assets/img/circle-200.png', h5: 'Aaron Dunn', p: 'johndoe.com', g: '“Fusce vehicula dolor arcu, sit amet \r\nblandit dolor mollis nec. Donec\r\n viverra eleifend lacus, vitae\r\n ullamcorper metus.”' },
      { img: 'assets/img/circle-200.png', h5: 'Scott Graham', p: 'Wall Street', g: '“Nam porttitor blandit accumsan. Ut\r\n vel dictum sem, a pretium dui. In\r\n malesuada enim in dolor euismod,\r\n id commodo mi consectetur.”' },
      { img: 'assets/img/circle-200.png', h5: 'Rachel Wells', p: 'Monster Inc.', g: '“Lorem ipsum dolor sit amet,\r\n consectetur adipiscing elit. Ut\r\n pretium pretium tempor. Ut eget\r\n imperdiet neque.” ' },
    ];
    for (let i = 0; i < 1; i++) {
      for (let j = 0; j < 4; j++) {
        img = new Item('img', {}, {
          'spanStyles': {
            'justify-content': 'center', 'border-radius': '200px', 'width': '197px',
            'height': '197px',
          }
        }, 1, [], [], '');
        img.attributes['src'] = source[counter].img
        h5 = new Item('h5', {}, { 'simpleStyles': { 'border': 'none', 'color': '#373742', 'font': 'normal normal normal 30px/34px Rubik', 'margin-top': '0px' } }, 2, [], [], source[counter].h5);
        para = new Item('p', {}, { 'simpleStyles': { 'border': 'none', 'color': '#7B7B88', 'font': 'normal normal normal 20px/19px Rubik' }, 'spanStyles': { 'width': '80%' } }, 3, [], [], source[counter].p);
        p = new Item('p', {}, { 'simpleStyles': { 'border': 'none', 'color': '#4F4F53', 'white-space': 'pre', 'font': 'italic normal normal 17px/28px Merriweather' }, 'spanStyles': { 'width': '80%' } }, 3, [], [], source[counter++].g);
        img.parentCol = grid2.rows[i].cols[j];
        h5.parentCol = grid2.rows[i].cols[j];
        para.parentCol = grid2.rows[i].cols[j];
        p.parentCol = grid2.rows[i].cols[j];
      }
    }
    this.currentPage.sections.push(section);
  }
