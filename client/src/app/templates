    this.lastCol.styles["background-image"] = "url('assets/img/joe-woods-4Zaq5xY5M_c-unsplash.jpg')"
    this.lastCol.styles["min-height"] = "100vh"
    let grid = DbService.createGrid(null, 1, 1);
    grid.parentCol = section.item.rows[0].cols[0];
    let h1 = new Item('h1', {}, {
      'simpleStyles': {
        'color': '#FBCD4F',
        'letter-spacing': '-1px',
        'opacity': '1',
        'border': 'none',
        'text-align': 'center',
        'font': 'normal normal 300 48px/62px Roboto'
      },
    }, null, [], [], 'What our Client Say');
    h1.parentCol = grid.rows[0].cols[0];
    let griddd = DbService.createGrid({ gridType: 'OneRowFourCol' });
    griddd.parentCol = grid.rows[0].cols[0]
    let img, h5, para, p;
    let counter = 0;
    let source = [
      { img: 'https://files.bldr.codes/uploads/shira0/img/1625036943384__manteam4.png', h5: 'Laura Poulsen', p: 'Creative Director', g: '“Lorem ipsum dolor sit amet, consectetur adipiscing elit. Ut pretium pretium tempor. Ut eget imperdiet neque.”' },
      { img: 'https://files.bldr.codes/uploads/shira0/img/1625036943384__manteam4.png', h5: 'Laura Poulsen', p: 'Creative Director', g: '“Lorem ipsum dolor sit amet, consectetur adipiscing elit. Ut pretium pretium tempor. Ut eget imperdiet neque.”' },
      { img: 'https://files.bldr.codes/uploads/shira0/img/1625036943384__manteam4.png', h5: 'Laura Poulsen', p: 'Creative Director', g: '“Lorem ipsum dolor sit amet, consectetur adipiscing elit. Ut pretium pretium tempor. Ut eget imperdiet neque.”' },
      { img: 'https://files.bldr.codes/uploads/shira0/img/1625036943384__manteam4.png', h5: 'Laura Poulsen', p: 'Creative Director', g: '“Lorem ipsum dolor sit amet, consectetur adipiscing elit. Ut pretium pretium tempor. Ut eget imperdiet neque.”' },
    ];
    for (let i = 0; i < 1; i++) {
      for (let j = 0; j < 4; j++) {
        img = new Item('img', {}, {
          'spanStyles': {
            'justify-content': 'center', 'border-radius': '200px', 'width': '197px',
            'height': '197px', 'border': '1px solid #FBD15B'
          }
        }, 1, [], [], '');
        img.attributes['src'] = source[counter].img

        h5 = new Item('h5', {}, { 'simpleStyles': { 'border': 'none', 'color': '#FBCD4F', 'font': 'normal normal normal 18px/21px Roboto', 'margin-top': '0px' } }, 2, [], [], source[counter].h5);
        para = new Item('p', {}, { 'simpleStyles': { 'border': 'none', 'color': '#595F6F', 'font': 'normal normal normal 14px/18px Roboto' }, 'spanStyles': { 'width': '80%' } }, 3, [], [], source[counter].p);
        p = new Item('p', {}, { 'simpleStyles': { 'border': 'none', 'color': '#595F6F', 'font': 'normal normal normal 14px/18px Roboto' }, 'spanStyles': { 'width': '80%' } }, 3, [], [], source[counter++].g);
        img.parentCol = griddd.rows[i].cols[j];
        h5.parentCol = griddd.rows[i].cols[j];
        para.parentCol = griddd.rows[i].cols[j];
        p.parentCol = griddd.rows[i].cols[j];
      }
    }
    this.currentPage.sections.push(section)
  }
