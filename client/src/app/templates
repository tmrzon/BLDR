create4_1Section() {
    let section = new Section(0, 'Customers Review', this.currentPage._id, '5fd7509f668c567a74a2a996');
    this.lastCol = section.item.rows[0].cols[0];
    let col = DbService.createGrid(null, 3, 1);
    section.item.styles.simpleStyles['background'] = 'linear-gradient(black 55%, white 45%)'
    col.parentCol = section.item.rows[0].cols[0];
    col.rows[0].cols[0].styles['display'] = 'flex'
    col.rows[0].cols[0].styles['flex-direction'] = 'column'
    col.rows[0].cols[0].styles['justify-content'] = 'center'
    let h2 = new Item('h2');
    Object.assign(h2.styles.simpleStyles, {
      'opacity': '1',
      'color': '#FBD15B',
      'font-size': '48px',
      'vertical-align': 'middle',
      'margin': '0px',
      'line-height': '0.8', 'width': '100%',
      'font-weight': '3',
      'min-height': '50px',
      'font': 'Roboto light', 'letter-spacing': '0.57px',
      'text-align': 'center', 'border': 'none',
    })
    h2.textContent.setValue('Discover our Store')
    h2.parentCol = col.rows[0].cols[0];
    let h2_2 = new Item('h2')
    Object.assign(h2_2.styles.simpleStyles, {
      'opacity': '1',
      'color': '#ffffff',
      'font-size': '15px',
      'vertical-align': 'middle',
      'margin': '0px',
      'line-height': '1.3', 'width': '100%',
      'font-weight': '3',
      'font': 'Roboto light', 'letter-spacing': '0.57px',
