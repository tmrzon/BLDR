        }
      }
    )
    btn1.textContent = 'BUY NOW'
    btn1.parentCol = grid.rows[0].cols[0];
    let btn2 = new Item('button', { 'class': 'btn float-right login_btn' },
      {
        'simpleStyles': {
          'color': '#333333',
          'background-color': '#FFFFFF',
          'width': '100%',
          'font': 'normal normal medium 16px/16px Rubik',
          'border-radius': '5px',
          'border-color': 'none',
          'min-height': '50px',
          'letter-spacing': '0px',
          'box-shadow': '20px 20px 60px #02081666',
        }
      }
    )
    btn2.textContent = 'MORE INFO'
    btn2.parentCol = grid.rows[0].cols[1];
    this.currentPage.sections.pop();
    this.currentPage.sections.push(section);
  }
