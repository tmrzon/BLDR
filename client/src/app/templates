CreateSection2000() {
    let section = new Section(0, 'My Services2', '', '5fd75031365f7f7a2fbbea62');
    this.lastCol = section.item.rows[0].cols[0];
    this.lastCol.styles = {
      'background-color': '#FFFFFF',
      'background-size': 'cover',
      'min-height': '600px'
    }
    let h1 = new Item('h2', { 'data-rellax-speed': '-1' },
      {
        'simpleStyles': {
          'opacity': '1',
          'color': '#000000',
          'font-size': '40px',
          'vertical-align': 'middle',
          'margin': '0px',
          'font-weight': 'bold',
          'line-height': '0.8', 'width': '100%',
          'font': 'Heavy 57px/56px SF Pro Display', 'letter-spacing': '0.57px',
          'text-align': 'center', 'border': 'none', 'min-height': '70px'
        },
        'spanStyles': {
          'align-items': 'center',
          'vertical-align': 'middle'
        }
      }, 0, [], [], "Pricing Table");
    h1.parentCol = section.item.rows[0].cols[0];
    let gridToBtn1 = DbService.createGrid({ gridType: 'OneRowThreeCol' });
    gridToBtn1.rows[0].cols[0].attributes['class'] = 'col-3';
    gridToBtn1.rows[0].cols[1].attributes['class'] = 'col-8'
    gridToBtn1.rows[0].cols[2].attributes['class'] = 'col-3'
    let btn0 = new Item('button', { 'class': 'btn float-right login_btn' },
      {
        'simpleStyles': {
          'color': '#ffffff',
          'background-color': '#4B266F',
          'font': 'normal normal medium 18px/34px Rubik',
          'top': '688px',
          'left': '618px',
          'width': '163px',
          'height': '58px',
          'border-radius': '0px',
          'position': 'center',
          'text-align': 'center',
          'box-shadow': '0px 2px 4px #00000080',
          'opacity': '1',
          'margin-left': '150px'
        }
      }
    )
    let gridToBtn = DbService.createGrid({ gridType: 'OneRowThreeCol' });
    gridToBtn.rows[0].cols[0].attributes['class'] = 'col-3';
    gridToBtn.rows[0].cols[1].attributes['class'] = 'col-8'
    gridToBtn.rows[0].cols[2].attributes['class'] = 'col-3'
    btn0.textContent = "Get Started";
    btn0.parentCol = gridToBtn.rows[0].cols[1];
    this.currentPage.sections.push(section);
  }
