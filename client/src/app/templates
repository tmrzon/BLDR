createEmailSection() {
    let section = new Section(0, 'Contacts', '', '5fd750b7668c567a74a2a997');
    this.lastCol = section.item.rows[0].cols[0];
    section.item.rows[0].cols[0].styles['height'] = 'fit-content';
    Object.assign(section.item.rows[0].cols[0].styles, { 'position': 'relative' });
    this.lastCol.styles = {
      'background-image': 'url(http://getwallpapers.com/wallpaper/full/a/5/d/544750.jpg)',
      'background-size': 'cover',
      'min-height': '600px',
      'align-content': 'center',
    }
    let gridToEmail = DbService.createGrid({ gridType: 'TwoRowOneCol' });
    gridToEmail.parentCol = section.item.rows[0].cols[0];
    let g = new Item('spacer', {}, { 'divStyles': { 'min-height': '5px' } })
    g.parentCol = gridToEmail.rows[0].cols[0];
    Object.assign(gridToEmail.rows[0].cols[0].styles, {
      'justify-content': 'center',
      'height': '100%',
    });
    let h2 = new Item('h2', { 'data-rellax-speed': '-1' },
      {
        'simpleStyles': {
          'opacity': '1',
          'color': '#f9faf5',
          'font-size': '57px',
          'vertical-align': 'middle',
          'margin': '0px',
          'line-height': '0.8', 'width': '100%',
          'font': 'bold 57px/56px SF Pro Display', 'letter-spacing': '0.57px',
          'text-align': 'center', 'border': 'none', 'min-height': '70px',
        },
        'spanStyles': {
          'align-items': 'center',
          'vertical-align': 'middle'
        }
      }, 0, [], [], "Subscibe to our Newsletter");
    let h5 = new Item('h5', { 'data-rellax-speed': '-1' },
      {
        'simpleStyles': {
          'opacity': '1',
          'color': '#f9faf5',
          'font-size': '20px',
          'vertical-align': 'middle',
          'margin': '0px',
          'line-height': '0.8', 'width': '100%',
          'font': 'bold 30px/50px SF Pro Display', 'letter-spacing': '0.57px',
          'text-align': 'center', 'border': 'none', 'min-height': '70px',
          'white-space': 'pre',
        },
        'spanStyles': {
          'align-items': 'center',
          'vertical-align': 'middle'
        }
      }, 0, [], [], "Join our newsletter and get news in your inbox every week! \r\n We hate spam too, so no worries about this.");
    h5.parentCol = gridToEmail.rows[0].cols[0];
    h2.parentCol = gridToEmail.rows[0].cols[0];
    let gridToLine = DbService.createGrid({ gridType: 'OneRowThreeCol' });
    gridToLine.parentCol = gridToEmail.rows[1].cols[0];
    gridToEmail.rows[0].cols[0].styles['height'] = 'fit-content';
    gridToLine.rows[0].cols[0].attributes['class'] = 'col-4';
    gridToLine.rows[0].cols[2].attributes['class'] = 'col-4';
    gridToLine.rows[0].cols[0].styles['min-height'] = '20px';
    gridToLine.rows[0].cols[1].styles['min-height'] = '20px';
    gridToLine.rows[0].cols[2].styles['min-height'] = '20px';
    let input = new Item('input', { 'class': 'form-control', 'placeholder': "Enter your email" },
      {
        'simpleStyles': {
          'color': '#000066',
        }
      }, 0, [], [], "Enter your email"
    )
    input.parentCol = gridToLine.rows[0].cols[1];
    let btn = new Item('button', { 'class': 'btn float-right login_btn' },
      {
        'simpleStyles': {
          'color': '#fcfcff',
          'background-color': '#323275',
          'width': '50%',
          'margin-right': '25%',
          'margin-left': '25%'
        }
      }
    )
    btn.textContent = "Subscribe";
    btn.parentCol = gridToLine.rows[0].cols[1];
    this.currentPage.sections.push(section)
  }
