createServicesSection() {
    let section = new Section(1, 'My Services', '', '5fd75031365f7f7a2fbbea62');
    this.lastCol = section.item.rows[0].cols[0];
    Object.assign(section.item.rows[0].cols[0].styles,
      {
        'background-image': 'linear-gradient(180deg, #FFFFFF 0%, #F5F6FA 100%)'
      });
    section.item.rows[0].cols[0].styles['height'] = 'fit-content';
    let gridToH2 = DbService.createGrid({ gridType: 'One' });
    gridToH2.parentCol = section.item.rows[0].cols[0];
    Object.assign(gridToH2.rows[0].cols[0].styles, { 'min-height': '80px' });
    let h2 = new Item('h2', { 'data-rellax-speed': '-1' },
      {
        'simpleStyles': {
          'opacity': '1',
          'color': '#1A223B',
          'font-size': '57px',
          'vertical-align': 'middle',
          'margin': '0px',
          'line-height': '0.8', 'width': '100%',
          'font': 'Heavy 57px/56px SF Pro Display', 'letter-spacing': '0.57px',
          'text-align': 'center', 'border': 'none', 'min-height': '70px'
        },
        'spanStyles': {
          'align-items': 'center',
          'vertical-align': 'middle'
        }
      }, 0, [], [], section.name);
    h2.parentCol = gridToH2.rows[0].cols[0];
    let gridToLine = DbService.createGrid({ gridType: 'OneRowThreeCol' });
    gridToLine.parentCol = gridToH2.rows[0].cols[0];
    gridToH2.rows[0].cols[0].styles['height'] = 'fit-content';
    gridToLine.rows[0].cols[0].attributes['class'] = 'col-5';
    gridToLine.rows[0].cols[2].attributes['class'] = 'col-5';
    Object.assign(gridToLine.rows[0].cols[1].styles, {
      'background': 'transparent linear-gradient(90deg, #4F63FF 0%, #4FF3FF 100%) 0% 0% no-repeat padding-box',
      'border-radius': '22px', 'height': '7px', 'min-height': '0px'
    })
