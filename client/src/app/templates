        h5.parentCol = grid.rows[i].cols[j];
        para.parentCol = grid.rows[i].cols[j];
        grid.rows[i].cols[j].attributes['class'] = "col-12 col-lg-4"
      }
    }
    this.currentPage.sections.push(section)
  }
